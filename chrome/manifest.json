{
  "name": "BbQuick",
  "version": "1.0",
  "description": "You'll be licking your fingers by the time you're done.",
  "browser_action": {
    "default_icon": "bb.png",
      "popup": "bbq.html"
  },
  "permissions": [
    "*://my.rochester.edu/*",
    "http://localhost:8000/*",
    "https://ajax.googleapis.com/*"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["*://my.rochester.edu/*"],
      "css": [],
      "js": ["jquery-1.6.4.js"],
      "all_frames": true
    }
  ],
  "content_security_policy": "default-src 'self' connect-src http://localhost:8000/ 'unsafe-inline'"
}
